# Owl Engine Configuration

# Met Office API Key (get from: https://register.metoffice.gov.uk/WaveRegistrationClient/public/register)
MET_OFFICE_API_KEY=your_api_key_here

# Data collection intervals (minutes)
HIGH_FREQUENCY_INTERVAL=5
MEDIUM_FREQUENCY_INTERVAL=30
LOW_FREQUENCY_INTERVAL=360

# Database settings
DATA_STORAGE_PATH=owl_data

# Traffic monitoring
TRAFFIC_COLLECTION_INTERVAL=5
